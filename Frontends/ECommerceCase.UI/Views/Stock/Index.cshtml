@model IEnumerable<ECommerceCase.UI.Models.StockViewModel>

<h2>Stock List</h2>
<table>
    <thead>
    <tr>
        <th>Product ID</th>
        <th>Name</th>
        <th>Stock</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ProductId</td>
            <td>@item.Name</td>
            <td>@item.Stock</td>
        </tr>
    }
    </tbody>
</table>